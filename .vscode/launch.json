  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (Template)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/Queil.Gmox.Template/template/bin/Debug/net6.0/Queil.Gmox.Template.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Launch (CLI)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/Queil.Gmox.DotNet.Cli/bin/Debug/net6.0/Queil.Gmox.DotNet.Cli.dll",
      "args": ["--proto",
        "tests/integration/protos/org/books/list/svc_list.proto",
        "--root",
        "tests/integration/protos",
        "--stub-dir",
        "tests/integration/stubs"],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}